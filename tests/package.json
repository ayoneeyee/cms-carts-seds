{
  "name": "tests",
  "version": "1.0.0",
  "description": "Automated Testing Framework using Nightwatch.js\r ## Table of Contents",
  "main": "nightwatch.js",
  "directories": {
    "lib": "lib",
    "test": "tests"
  },
  "dependencies": {
    "chromedriver": "^86.0.0",
    "geckodriver": "^1.20.0",
    "nightwatch": "^1.5.1"
  },
  "devDependencies": {},
  "scripts": {
    "chrome": "nightwatch -c conf/nightwatch.conf.js",
    "chrome-win": "node nightwatch -c conf/nightwatch.conf.js",
    "firefox": "nightwatch -c conf/nightwatch.conf.js -e firefox",
    "firefox-win": "node nightwatch -c conf/nightwatch.conf.js -e firefox",
    "test": "nightwatch -c conf/nightwatch.conf.js -e chrome, firefox",
    "test-win": "node nightwatch -c conf/nightwatch.conf.js -e default, firefox",
    "update": "npx npm-check-updates -u && npm install"
  },
